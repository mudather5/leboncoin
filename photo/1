                scrollToObject( $('label[for="address"]') );
            }
        };
    }

    newadFormSpy = new Form('#formular', '#formular .input, #formular .nude, #formular .textarea, #formular .select', fieldObject, true);

    $('body').on('submit', '#formular', function(e) {
        var returnValue = true;
        e.stopImmediatePropagation();
        $('span.label-error.visible').removeClass('visible');

        if (typeof($(this).attr('target')) == 'undefined') {
            if (newadFormSpy.check()) {
                if (newadForm.showLocationError('location_p') == false) returnValue = false;
                if (newadForm.checkAnimal("Veuillez s&eacute;lectionner un type d'animal.", "Veuillez indiquer la nature de l'offre (vente, don ou saillie).", "Veuillez indiquer si l'animal appartient &agrave; une race ou non.", "Pour proposer &agrave; la vente des chiots ou chatons qui ne sont pas enregistr&eacute;s aux livres g&eacute;n&eacute;alogiques, veuillez vous enregistrer en tant qu'&eacute;leveur professionnel.", "Veuillez indiquer le nombre d'animaux dans la port&eacute;e.", "Veuillez indiquer le nombre d'animaux dans la port&eacute;e en chiffre.", "Veuillez indiquer l'&acirc;ge de l'animal.", "Selon la r&eacute;glementation en vigueur, un particulier ne peut pas vendre plus d'une port&eacute;e par an par foyer fiscal. Dans le cas &eacute;ch&eacute;ant, nous vous invitons &agrave; vous cr&eacute;er un compte pro muni de votre SIREN d'&eacute;leveur.", "Veuillez indiquer le prix correspondant &agrave; votre annonce.", "Veuillez indiquer si votre animal est tatou&eacute; ou puc&eacute;.", "Pour proposer &agrave; la vente des chiens ou chats adultes, veuillez vous enregistrer en tant qu'&eacute;leveur professionnel.", "Veuillez indiquer le num&eacute;ro de port&eacute;e de l'animal.", "Il semblerait que le num&eacute;ro de port&eacute;e renseign&eacute; soit erron&eacute;.", "Veuillez indiquer le num&eacute;ro d'identification de l'animal ou de la m&egrave;re si l'animal est trop jeune pour avoir le sien.", "Il semblerait que le num&eacute;ro d'identification renseign&eacute; soit erron&eacute;.") == false) returnValue = false;

                return returnValue;
            } else {
                returnValue = false;
                newadForm.showLocationError('location_p')
                newadForm.checkAnimal("Veuillez s&eacute;lectionner un type d'animal.", "Veuillez indiquer la nature de l'offre (vente, don ou saillie).", "Veuillez indiquer si l'animal appartient &agrave; une race ou non.", "Pour proposer &agrave; la vente des chiots ou chatons qui ne sont pas enregistr&eacute;s aux livres g&eacute;n&eacute;alogiques, veuillez vous enregistrer en tant qu'&eacute;leveur professionnel.", "Veuillez indiquer le nombre d'animaux dans la port&eacute;e.", "Veuillez indiquer le nombre d'animaux dans la port&eacute;e en chiffre.", "Veuillez indiquer l'&acirc;ge de l'animal.", "Selon la r&eacute;glementation en vigueur, un particulier ne peut pas vendre plus d'une port&eacute;e par an par foyer fiscal. Dans le cas &eacute;ch&eacute;ant, nous vous invitons &agrave; vous cr&eacute;er un compte pro muni de votre SIREN d'&eacute;leveur.", "Veuillez indiquer le prix correspondant &agrave; votre annonce.", "Veuillez indiquer si votre animal est tatou&eacute; ou puc&eacute;.", "Pour proposer &agrave; la vente des chiens ou chats adultes, veuillez vous enregistrer en tant qu'&eacute;leveur professionnel.", "Veuillez indiquer le num&eacute;ro de port&eacute;e de l'animal.", "Il semblerait que le num&eacute;ro de port&eacute;e renseign&eacute; soit erron&eacute;.", "Veuillez indiquer le num&eacute;ro d'identification de l'animal ou de la m&egrave;re si l'animal est trop jeune pour avoir le sien.", "Il semblerait que le num&eacute;ro d'identification renseign&eacute; soit erron&eacute;.")
            }
            return returnValue;
        }
    });

    $('body').on('blur', '#formular .input, #formular .nude, #formular .textarea, #formular .select', function() {
        var selector = this.tagName.toLowerCase() + '[name="' + $(this).attr('name') + '"]';
        return newadFormSpy.check(selector);
    });

    $('body').on('blur', 'input[name="availability_price"]', function() {
        var fieldValue = $(this).val();
        var pattern = new RegExp(/^([0-9]*)$/i);
        if (!(pattern.test(fieldValue)))
            $('span.label-error[data-for="datespickers"]').html("Le prix contient des caract&egrave;res non autoris&eacute;s.").addClass('visible');
        else
            $('span.label-error[data-for="datespickers"]').html("").removeClass('visible');
    });
</script>

