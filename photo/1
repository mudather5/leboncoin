            $("body").on("focusin", "#body", function() {
                var labelInfoBody = $('span.label-info[data-for="body"]').html();
                if ($("#private_ad_id").is(":checked") && $("#category").val()==6)
                    $('span.label-info[data-for="body"]').html("Les voitures non roulantes et/ou accident&eacute;es ne peuvent &ecirc;tre vendues et doivent &ecirc;tre remises dans des centres VHU agr&eacute;&eacute;s. La vente d'&eacute;l&eacute;ment pyrotechnique est &eacute;galement interdite aux particuliers (airbags, etc.). Indiquez dans l'annonce si vous d&eacute;sirez &ecirc;tre contact&eacute;(e) uniquement par t&eacute;l&eacute;phone.");
                else {
                    $('span.label-info[data-for="body"]').html(labelInfoBody);
