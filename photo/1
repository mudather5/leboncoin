<script type="text/javascript">
    function loadCookieBannerTracking() {
        if (typeof window.trackXiti === "function") {
            window.trackXiti({"campaign":"bandeau_cookies", "event_type":"publisher_impression", "creation": "ouverture", "variant":"newad"});
        } else {
            setTimeout(loadCookieBannerTracking, 500);
        }
    }

    var cookieBannerName = 'hideCookieFrame';
    if(Date.now() > 1527199200000) {
        cookieDefault.removeCookie(cookieBannerName, ".leboncoin.fr");
        cookieBannerName = 'cookieBanner';
    }

	if(cookieDefault.getCookie(cookieBannerName) == 1) {
        document.getElementById('cookieBanner').parentNode.removeChild(document.getElementById('cookieBanner'));
	} else {

        $('.cookieMore').click(function(){
            event.stopPropagation();
            cookieDefault.addCookie(cookieBannerName, 1, 395, ".leboncoin.fr");
            cookieDefault.addCookie('consent_geo', 0, 395, ".leboncoin.fr");
            cookieDefault.addCookie('consent_comp', 1, 395, ".leboncoin.fr");
            cookieDefault.addCookie('consent_cookie', 0, 395, ".leboncoin.fr");
            document.getElementById('cookieBanner').parentNode.removeChild(document.getElementById('cookieBanner'));
        })

        $('*').not('.cookieMore').bind('click.cookieBanner', function(e) {
            cookieDefault.addCookie(cookieBannerName, 1, 395, ".leboncoin.fr");
            cookieDefault.addCookie('consent_geo', 1, 395, ".leboncoin.fr");
            cookieDefault.addCookie('consent_comp', 1, 395, ".leboncoin.fr");
            cookieDefault.addCookie('consent_cookie', 1, 395, ".leboncoin.fr");
            document.getElementById('cookieBanner').parentNode.removeChild(document.getElementById('cookieBanner'));
            $('*').unbind('click.cookieBanner');
        })

        // tracking
        loadCookieBannerTracking()
	}
</script>

